<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid name="hiringcomapny-grid" title="Workers" model="com.axelor.apps.base.db.HiringCompany">
		<field name="company" />
		<field name="city" />
	</grid>

	<form name="hiringcomapny-form" title="Workers" model="com.axelor.apps.base.db.HiringCompany">
		<panel name="mainPanel">
			<field name="company" />
			<field name="city" />
			
		</panel>
		<panel name="workerslist" title="Workers Details">
			<panel-related field="workersLine" title="Workers List"
				colSpan="12" grid-view="workersline-grid" form-view="workersline-form"/>
		</panel>
	</form>

	<action-record name="action-record-workers-values-1" model="com.axelor.apps.base.db.Workers"
		search="self.name = 'abhinav'" copy="true">
		<field name="name" expr="abhinav" />
		<field name="city" expr="surat" />
	</action-record>
	<!-- expr="eval: &quot; ${__user__.id} MEMBER OF self.membersUserSet OR self.assignedTo.id = ${__user__.id} &quot;" -->
	<action-attrs name="action-attr-filters-currency-1">
			<attribute name="domain" for="currency" expr="eval: &quot; self.currency.id = { 64 } &quot;"/>
	</action-attrs>
	
	<action-attrs name="action-attr-filters-hobbies-1">
			<attribute name="domain" for="hobbiesSet" expr="eval: &quot; ${1,2} MEMBER OF self.hobbiesSet &quot;"/>
	</action-attrs>
	

</object-views>